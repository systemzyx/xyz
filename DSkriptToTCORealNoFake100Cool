				task.wait()
			end
		end)
	end
})
local crashActive = false

Tab:AddToggle({
	Name = "Crash Player",
	Default = false,
	Callback = function(Value)
		crashActive = Value

		if crashActive then
			local startTime = tick()
			local rLoopActive = false

			while crashActive do
				if selectedPlayer then
					local name = getPlayerSafeName(selectedPlayer)
					say("food " .. name)

					if tick() - startTime >= 40 then
						rLoopActive = true
					end

					if rLoopActive then
						say("r6 " .. name)
						task.wait(0.1)
						say("r15 " .. name)
						task.wait(0.1)
					end
				end

				task.wait(0.01)
			end
		end
	end
})
