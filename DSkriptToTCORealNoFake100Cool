local antiCrashActive = false
Tab:AddToggle({
	Name = "Anti Crash",
	Default = true,
	Callback = function(Value)
		antiCrashActive = Value
		local foods = {"Cheezburger", "Taco", "BloxyCola", "Watermelon", "Pizza"}
		local function anti()
			local backpack = LocalPlayer:FindFirstChild("Backpack")
			if backpack then
				for _, v in pairs(backpack:GetDescendants()) do 
					if v:IsA("Tool") and table.find(foods, v.Name) then 
						v:Destroy()
					end
				end
			end
		end
		task.spawn(function()
			while antiCrashActive do
				anti()
				task.wait()
			end
		end)
	end
})

local enlightenActive = false
Tab:AddToggle({
	Name = "Permanent Enlighten",
	Default = true,
	Callback = function(Value)
		enlightenActive = Value
		local toolName = "The Arkenstone"
		local function equip()
			local backpack = LocalPlayer:FindFirstChild("Backpack")
			local character = LocalPlayer.Character
			local toolInBackpack = backpack and backpack:FindFirstChild(toolName)
			if character and toolInBackpack then
				toolInBackpack.Parent = character
			end
		end
		local function check()
			local character = LocalPlayer.Character
			local backpack = LocalPlayer:FindFirstChild("Backpack")
			if not character or not backpack then return end
			local toolInChar = character:FindFirstChild(toolName)
			local toolInBackpack = backpack:FindFirstChild(toolName)
			if toolInChar then
				say(";reset me")
				task.wait(0.1)
			elseif toolInBackpack then
				equip()
			end
		end
		task.spawn(function()
			while enlightenActive do
				check()
				task.wait()
			end
		end)
	end
})

local ChatTab = Window:MakeTab({Name = "??", Icon = "0", PremiumOnly = false})
local Section = ChatTab:AddSection({
	Name = "Chat Logs - Spy"
})
local chatLabels = {}
local chatMax = 200

local function addChatLabel(msg)
	local label = ChatTab:AddLabel(msg)
	table.insert(chatLabels, label)
	if #chatLabels > chatMax then
		chatLabels[1]:Destroy()
		table.remove(chatLabels, 1)
	end
end

task.spawn(function()
	repeat task.wait() until TextChatService:FindFirstChild("TextChannels")
	local general = TextChatService.TextChannels:FindFirstChild("RBXGeneral")
	if general then
		general.MessageReceived:Connect(function(message)
			local src = message.TextSource
			if src and src.UserId ~= LocalPlayer.UserId then
				local player = Players:GetPlayerByUserId(src.UserId)
				if player then
					local time = os.date("%H:%M:%S")
					addChatLabel("[" .. time .. "] [" .. player.Name .. "]: " .. message.Text)
				end
			end
		end)
	end
end)
