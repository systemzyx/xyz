local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/systemzyx/CustomOrion/refs/heads/main/simple"))()
local Window = OrionLib:MakeWindow({Name = "Quantum XV - Reborn", HidePremium = false, SaveConfig = false})

local Players = game:GetService("Players")
local TextChatService = game:GetService("TextChatService")
local LocalPlayer = Players.LocalPlayer

function say(text)
	game.Players:Chat(text)
end

local function findPlayer(name)
	for _, player in pairs(Players:GetPlayers()) do
		if string.find(string.lower(player.Name), string.lower(name)) or string.find(string.lower(player.DisplayName), string.lower(name)) then
			return player
		end
	end
	return nil
end

local function getPlayerSafeName(player)
	local display = player.DisplayName
	local name = player.Name

	if not string.find(display, "_") then
		return display
	elseif not string.find(name, "_") then
		return name
	else
		local short = name:split("_")[1]
		return short
	end
end

local Tab = Window:MakeTab({Name = "?", Icon = "0", PremiumOnly = false})
Tab:AddSection({Name = "Section - Textbox"})

local selectedLabel = Tab:AddLabel("Selected : N/A")
local selectedPlayer = nil

Tab:AddTextbox({
	Name = "Target Player",
	Default = "",
	TextDisappear = true,
	Callback = function(input)
		selectedPlayer = findPlayer(input)
		if selectedPlayer then
			selectedLabel:Set("Selected : " .. selectedPlayer.DisplayName)
		else
			selectedLabel:Set("Player not found")
		end
	end
})

Tab:AddSection({Name = "Section - Buttons"})

Tab:AddButton({
	Name = "UnEnlighten Player",
	Callback = function()
		if selectedPlayer then
			local target = getPlayerSafeName(selectedPlayer)
		   	say("enlighten " .. target)
			task.wait(0.5)
			say("clearinv " .. target)
			task.wait(0.2)
			say("clearinv " .. target)
		end
	end
})

Tab:AddButton({
	Name = "Embarass Player",
	Callback = function()
		if selectedPlayer then
			local target = getPlayerSafeName(selectedPlayer)
			say("morph " .. target .. " heart0heart")
			wait(1.5)
			say("name " .. target .. " gay")
		end
	end
})

Tab:AddButton({
	Name = "Punish Player",
	Callback = function()
		if selectedPlayer then
			local target = getPlayerSafeName(selectedPlayer)
			say("glitch " .. target)
			wait()
