		end
	end
})
Tab:AddButton({
	Name = "Steal B-Kit",
	Callback = function()
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")

local priorityOrder = { "Build", "Delete", "Paint" }
local toolNames = {
	Build = true,
	Delete = true,
	Paint = true,
	Sign = true,
	Shape = true,
	Shovel = true
}

local collectedTools = {}
local maxTools = 6

local function collectTools()
	local toolsToGet = {}
	local count = 0
	local selectedOwner = nil

	local function tryAddTool(tool, owner)
		if not collectedTools[tool] and count < maxTools then
			table.insert(toolsToGet, { tool = tool, owner = owner })
			count += 1 --
		end
	end

	local function findQualifiedOwner()
		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= player then
				local found = {}
				for _, item in ipairs(plr:GetDescendants()) do
					if item:IsA("Tool") and table.find(priorityOrder, item.Name) then
						found[item.Name] = true
					end
				end
				local hasAll = true
				for _, requiredTool in ipairs(priorityOrder) do
					if not found[requiredTool] then
						hasAll = false
						break
					end
				end
				if hasAll then
					return plr
				end
			end
		end
		return nil
	end

	local function scanForTools()
		selectedOwner = findQualifiedOwner()
		if not selectedOwner then return end
		for _, item in ipairs(selectedOwner:GetDescendants()) do
			if item:IsA("Tool") and toolNames[item.Name] then
				tryAddTool(item, selectedOwner)
			end
		end
		for _, item in ipairs(workspace:GetDescendants()) do
			if item:IsA("Tool") and toolNames[item.Name] then
				local model = item:FindFirstAncestorWhichIsA("Model")
				local owner = Players:GetPlayerFromCharacter(model)
				if owner == selectedOwner then
					tryAddTool(item, owner)
				end
			end
		end
	end

	scanForTools()
	for _, entry in ipairs(toolsToGet) do
		entry.tool.Parent = backpack
		collectedTools[entry.tool] = true
	end
end

local function destroyTools()
	for tool in pairs(collectedTools) do
		if tool and tool.Parent then
			tool:Destroy()
		end
	end
	collectedTools = {}
end

local function onCharacterAdded(char)
	char:WaitForChild("Humanoid").Died:Connect(function()
		destroyTools()
	end)
	task.delay(1, function()
		collectTools()
	end)
end

if player.Character then
	onCharacterAdded(player.Character)
end
player.CharacterAdded:Connect(onCharacterAdded)
collectTools()
	end
})
Tab:AddSection({Name = "Section - Toggles"})
Tab:AddToggle({
	Name = "Freefall Player",
	Default = false,
	Callback = function(Value)
		isTre = Value
		task.spawn(function()
