	wait()
end
	end    
})
function noAirstrike()
       if game.Workspace:FindFirstChild("Rocket") or game.Workspace:FindFirstChild("Part") then 
          wait(0.3)
        say("debug")
        wait(0.3)
         say("god me")
   end
end
prot:AddToggle({
	Name = "Anti Rocket & Missiles",
	Default = false,
	Callback = function(args)
		noStrike = args
     while noStrike do 
	    noAirstrike()
	wait()
	    end
	end    
})
