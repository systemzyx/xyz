-- Fucntion
function antiTrip()
        local subspace = game:GetService("Workspace"):FindFirstChild("SubspaceTripmine")
if subspace then
            local player = game.Players.LocalPlayer
        local tool = player.Backpack:FindFirstChild("The Arkenstone")
        if tool then
            tool.Parent = player.Character
         end
          wait(0.7)
        say("debug")
        wait(0.7)
         say("god me")
   end
end
prot:AddToggle({
	Name = "Anti Subspace Tripmine.",
	Default = false,
	Callback = function(state)
		antisub = state
		while antisub do 
	 antiTrip()
	wait()
end
	end    
})
function antiFuse()
        local fuse = game:GetService("Workspace"):FindFirstChild("FuseBomb")
if fuse then
        say("debug")
        wait(0.7)
         say("god me")
   end
end
prot:AddToggle({
	Name = "Anti Fuse Bomb (Bomb)",
	Default = false,
	Callback = function(state)
		myFuse = state 
