	Name = "Anti Abused (OP)",
	Default = false,
	Callback = function(state)
		AAbused = state 
		while AAbused do 
	       abused()
	    wait()
	    end
	end    
})
function enAbused() 
       local myc = game.Players.LocalPlayer.Character 
       local getserv = game:GetService("Players").LocalPlayer
if myc:FindFirstChild("Hielo") or myc:FindFirstChild("Jail") then 
        myc:FindFirstChild("Humanoid").Health = 0 
       getserv.Character:FindFirstChild("Jail"):Destroy()
     end
end
